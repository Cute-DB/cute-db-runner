        <run>false</run>
        <table-exclusion-pattern><![CDATA[\w*\.((databasechangeloglock)|(databasechangelog)|(\w*_aud)|(revinfo))]]></table-exclusion-pattern>
        <config>
            <property name="message">message for SQL catalog lint</property>
